{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "program": "${workspaceFolder}/src/main.js",
        "skipFiles": [
          "<node_internals>/**"
        ]
      },
      {
        "type": "node",
        "request": "attach",
        "protocol": "inspector",
        "name": "Attach",
        "port": 9229,
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
}
